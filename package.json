{
	"name": "@ylide/sdk",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"version": "0.8.0-beta.5",
	"description": "Ylide Protocol SDK for cross-chain communication and messaging",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/ylide-io/sdk.git"
	},
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"test": "mocha -r ts-node/register 'src/tests/index.ts'",
		"dry-run": "ts-node src/__tests__/index.ts",
		"build": "rimraf ./lib && tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix",
		"prepublishOnly": "npm run lint",
		"preversion": "npm run lint",
		"docgen": "typedoc src/index.ts --name \"Ylide SDK\" --requiredToBeDocumented Class --out docs --plugin typedoc-plugin-coverage --excludeInternal"
	},
	"keywords": [
		"ylide",
		"messaging",
		"communication",
		"blockchain",
		"everscale",
		"ethereum"
	],
	"author": "Danila Simonov",
	"license": "ISC",
	"devDependencies": {
		"@types/brotli": "1.3.1",
		"@types/chai": "4.3.4",
		"@types/crypto-js": "4.1.1",
		"@types/deep-equal": "1.0.1",
		"@types/mocha": "10.0.1",
		"@types/node": "18.15.5",
		"@types/pako": "2.0.0",
		"@types/sjcl": "1.0.30",
		"@typescript-eslint/eslint-plugin": "5.56.0",
		"@typescript-eslint/parser": "5.56.0",
		"chai": "4.3.7",
		"eslint": "8.36.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-jsdoc": "39.6.8",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"mocha": "10.2.0",
		"prettier": "2.8.6",
		"rimraf": "4.4.1",
		"ts-node": "10.9.1",
		"typedoc": "0.23.24",
		"typedoc-plugin-coverage": "2.0.0",
		"typescript": "5.0.2"
	},
	"dependencies": {
		"@datastructures-js/binary-search-tree": "5.2.0",
		"@datastructures-js/linked-list": "5.2.5",
		"@ylide/smart-buffer": "0.0.11",
		"eventemitter3": "5.0.0",
		"idb": "7.1.1",
		"js-sha256": "0.9.0",
		"pako": "2.1.0",
		"tweetnacl": "1.0.3"
	}
}
